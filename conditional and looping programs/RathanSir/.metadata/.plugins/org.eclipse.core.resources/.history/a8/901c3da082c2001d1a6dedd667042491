package com.sathya.cookey;

import java.io.IOException;
import java.io.PrintWriter;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.Cookie;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

/**
 * Servlet implementation class successServlet
 */
@WebServlet("/successServlet")
public class successServlet extends HttpServlet {
	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		response.setContentType("text/html");
		PrintWriter writer = response.getWriter();
		
		Cookie[]cookies= request.getCookies();
				
		writer.println(cookies[0].getValue());
		writer.println(cookies[1].getValue());
		writer.println(cookies[2].getValue());
		writer.println(cookies[3].getValue());
		writer.println(cookies[4].getValue());
		writer.println(cookies[5].getValue());
	}
}
