package com.sathya.operations;

public class Operations {
	static int num1=10;
	static int num2=20;
	public void add() {
		int add;
		add=num1+num2;
		System.out.println("Addition of two numbers is--"+add);
	}
	public static void mul() {
		int mul;
		mul=num1*num2;
		System.out.println("multiplication of two number--"+mul);
	}

}
