import java.util.Scanner;
public class RathanSir {

	public static void main(String[] args) {
		String number="10";
		while(true)
		{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter any thing");
		String any=sc.next();
		if(number==any)
		System.out.println("Rathan Sir is Hero");
		break;
		else
			System.out.println("Rathan Sir is Hero");
			
		}
	}
}
