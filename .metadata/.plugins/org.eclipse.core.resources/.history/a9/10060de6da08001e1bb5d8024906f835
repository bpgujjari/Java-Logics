package com.logics;
public class PatternPrint {
    public static void main(String[] args) {
        int rows = 6;
        int cols = 7;
        
        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < cols; j++) {
                if (i % 2 == 0) {
                    System.out.print("+");
                } else {
                    System.out.print("*");
                }
            }
            System.out.println();
        }
    }
}