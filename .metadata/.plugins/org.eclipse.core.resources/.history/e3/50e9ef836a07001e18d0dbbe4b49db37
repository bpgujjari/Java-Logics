package com.logics;

public class Missing {
public static void main(String[] args) {
	int[]arr= {1,2,3,5,6,7};
	int largest=arr[0],smallest=arr[0];
	int sum1,sum2=0;
	
	for(int i=1;i<arr.length;i++)
	{
		if(smallest>arr[i])
		{
			smallest=arr[i];
			
		}
		if(largest<arr[i])
		{
			largest=arr[i];
			
		}
	}
	System.out.println(smallest+"  "+largest);
}
}
